var title = vx.ww.document.querySelector(".featured-video.distroscale .control-heading--max");
if(title){
title.className += " " + "ds_title";
}
(function() {
    var referenceElem = vx.ww.document.querySelector(".featured-video.distroscale .");
    var elemToMove = vx.ww.document.getElementById("ds_content_frame_parent");
    var cpp = vx.ww.document.getElementById("ds_cpp");




    function checkInview(msg) {
        var refElemRT = referenceElem.getBoundingClientRect();
        var cppRT = cpp.getBoundingClientRect();
        if (refElemRT.width > cppRT.width + 20) {
            dock();

        } else {
            undock();
        }



    }

    init();

    function init() {


        checkInview();
        setInterval(function() {
            checkInview();
        }, 1000);
        vx.addListener(vx.ww, "scroll", checkInview);
    }


    function dock() {



        var cppRT = cpp.getBoundingClientRect();


        elemToMove.style.width = cppRT.width;
        elemToMove.style.height = cppRT.height;
        var actionDivEl = elemToMove.querySelectorAll("div");
        for (var i = 0; i < actionDivEl.length; i++) {
            if (actionDivEl[i].id.length == 0 && actionDivEl[i].className.length == 0) {
                actionDivEl[i].style.height = cppRT.height - 32 + "px";
                break;
            }
        }
        var adEl = vx.ww.document.getElementById("ds_ad_container");
        if (adEl) {
            adEl.style.width = cppRT.width;
            adEl.style.height = cppRT.height;
            if (vx.adManager && vx.adManager.activeAd && vx.adManager.activeAd.ifr) {
                vx.adManager.sendMessage(vx.adManager.activeAd.ifr, "resizeMe", {
                    "width": cppRT.width,
                    "height": cppRT.height
                });
            }
        }
        var ctEl = vx.ww.document.getElementById("ds_content_frame");
        if (ctEl) {
            ctEl.style.width = cppRT.width;
            ctEl.style.height = cppRT.height;
        }
    }

    function undock() {

        var refElemRT = referenceElem.getBoundingClientRect();


        elemToMove.style.boxSizing = "border-box";

        elemToMove.className = elemToMove.className ? elemToMove.className.replace(new RegExp('(^|\\b)(ds_float)(\\b|$)', 'gi'), ' ') : '';
        elemToMove.className = elemToMove.className ? elemToMove.className.replace(new RegExp('(^|\\b)(tl|tr|bl|br|ct)(\\b|$)', 'gi'), ' ') : '';
        elemToMove.style.width = refElemRT.width
        elemToMove.style.height = refElemRT.height

        var actionDivEl = elemToMove.querySelectorAll("div");
        for (var i = 0; i < actionDivEl.length; i++) {
            if (actionDivEl[i].id.length == 0 && actionDivEl[i].className.length == 0) {
                actionDivEl[i].style.height = vx.dvp_height - 32 + "px";
                break;
            }
        }
        var adEl = vx.ww.document.getElementById("ds_ad_container");
        if (adEl) {
            adEl.style.width = refElemRT.width;
            adEl.style.height = refElemRT.height;
            if (vx.adManager && vx.adManager.activeAd && vx.adManager.activeAd.ifr) {
                vx.adManager.sendMessage(vx.adManager.activeAd.ifr, "resizeMe", {
                    "width": refElemRT.width,
                    "height": refElemRT.height
                });
            }
        }
        var ctEl = vx.ww.document.getElementById("ds_content_frame");
        if (ctEl) {
            ctEl.style.width = refElemRT.width;
            ctEl.style.height = refElemRT.height;
        }
    }
})();
